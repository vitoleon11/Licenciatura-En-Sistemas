programa gork
procesos
  proceso hacerLado
  comenzar
    repetir 20
      mover
  fin
  

areas
  ciudad: AreaC (1,1,100,100)
  
  
robots
  robot tipo1
  comenzar
    hacerLado
  fin
  
  
  robot tipo2
  comenzar
    derecha
    hacerLado
  fin
  
  robot tipo3
  comenzar
    repetir 2
      derecha
    hacerLado
  fin
  
  
  robot tipo4
  comenzar
    repetir 3
      derecha
    hacerLado
  fin
  
variables
  robot1: tipo1
  robot2: tipo2
  robot3: tipo3
  robot4 : tipo4
comenzar
  AsignarArea(robot1, ciudad)
  AsignarArea(robot2, ciudad)
  AsignarArea(robot3, ciudad)
  AsignarArea(robot4, ciudad)
  Iniciar(robot1, 10,10)
  Iniciar(robot2, 10,30)
  Iniciar(robot3, 30,30)
  Iniciar(robot4, 30,10)
